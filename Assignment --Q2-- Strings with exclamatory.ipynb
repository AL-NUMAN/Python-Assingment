    "def main():\n",
    "    punctuations = '!'\n",
    "    print('Enter your name:')\n",
    "    x = raw_input()\n",
    "    my_str = x\n",
    "    no_punct = \"\"\n",
    "    for char in my_str:\n",
    "        if char not in punctuations:\n",
    "            no_punct = no_punct + char\n",
    "\n",
    "\n",
    "    print(no_punct)\n",
    "main()\n"
   ]
